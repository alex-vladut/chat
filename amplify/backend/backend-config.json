{
  "auth": {},
  "api": {
    "chat": {
      "service": "AppSync",
      "providerPlugin": "awscloudformation",
      "output": {
        "authConfig": {
          "additionalAuthenticationProviders": [],
          "defaultAuthentication": {
            "authenticationType": "API_KEY",
            "apiKeyConfig": {
              "description": "",
              "apiKeyExpirationDays": 7
            }
          }
        }
      }
    }
  },
  "function": {
    "SendSMS": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "api",
          "resourceName": "chat",
          "attributes": ["GraphQLAPIIdOutput", "GraphQLAPIEndpointOutput"]
        }
      ]
    }
  },
  "custom": {
    "sms": {
      "service": "SNSAndSQS",
      "providerPlugin": "awscloudformation"
    }
  }
}
